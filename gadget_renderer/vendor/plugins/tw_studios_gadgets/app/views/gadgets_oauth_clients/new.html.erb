<h1>New gadgets_oauth_client</h1>

<% form_for(@gadgets_oauth_client) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :gadget_url %><br />
    <%= f.text_field :gadget_url %>
  </p>
  <p>
    <%= f.label :client_id, 'Client id' %><br />
    <%= f.text_field :client_id %>
  </p>
  <p>
    <%= f.label :client_secret %><br />
    <%= f.text_field :client_secret %>
  </p>
  <p>
    <%= f.label :service_name %><br />
    <%= f.text_field :service_name %>
  </p>
  <p>
    <%= f.label :redirect_uri %><br />
    <%= f.text_field :redirect_uri %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', gadgets_oauth_clients_path %>